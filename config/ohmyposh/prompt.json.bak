{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "{{ if .Root }}❯❯{{ else }}❯❯{{ end }} ",
    "foreground_templates": ["{{ if .Root }}#e5c890{{ else }}#8caaee{{ end }}"],
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "❯ ",
    "background": "transparent",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#e78284{{ end }}",
      "{{ if and (eq .Code 0) .Root }}#e5c890{{ end }}",
      "{{ if and (eq .Code 0) (not .Root) }}#8caaee{{ end }}"
    ]
  },
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "console_title_template": "Terminal",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "template": " {{ .UserName }}",
          "foreground_templates": [
            "{{ if .Root }}#e78284{{ else }}#8caaee{{ end }}"
          ],
          "background": "transparent"
        },
        {
          "type": "session",
          "style": "plain",
          "template": "<#a5adce> ❘ </>󰇄 {{ .HostName }}<#a5adce> ❘ </>",
          "foreground": "#8caaee",
          "background": "transparent"
        },
        {
          "type": "path",
          "style": "plain",
          "properties": {
            "style": "full"
          },
          "template": " {{ .Path }}",
          "foreground": "#8caaee",
          "background": "transparent"
        },
        {
          "type": "git",
          "style": "plain",
          "properties": {
            "branch_icon": "\uf408 \ue0a0 ",
            "commit_icon": "@",
            "fetch_status": true
          },
          "template": " {{ .HEAD }}{{ if or (.Working.Changed) (.Staging.Changed) }}*{{ end }} <#a6d189>{{ if gt .Behind 0 }}\u21e3{{ end }}{{ if gt .Ahead 0 }}\u21e1{{ end }}</>",
          "foreground": "#a5adce",
          "background": "transparent"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "newline": false,
      "filler": " ",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "template": "{{ if or .Env.SSH_CONNECTION .Env.SSH_CLIENT }}\ueb3a SSH{{ end }}",
          "foreground": "#a6d189",
          "background": "transparent"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "❯",
          "background": "transparent",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Root }}#e5c890{{ else }}#8caaee{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "final_space": true
}
